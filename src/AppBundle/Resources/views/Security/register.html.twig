{% extends '::base.html.twig' %}
{% block body %}
    {% if error %}
        <div class="alert alert-dismissible alert-danger col-md-5 text-right">
            <strong>Oh snap!</strong> {{ error.messageKey|trans(error.messageData, 'security') }}
        </div>
    {% endif %}
    <div class="col-md-3 well">
        {{ form(form) }}
    </div>


{% endblock %}
